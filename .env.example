cat > .env.example << 'EOF'
# Discord Bot Token (dari Discord Developer Portal)
DISCORD_TOKEN=your_bot_token_here

# Bot Prefix
PREFIX=!

# Optional: 2Captcha API Key (untuk solve captcha)
CAPTCHA_API_KEY=

# Optional: Allowed Channels (comma separated IDs)
ALLOWED_CHANNELS=

# Port for health check (Render requirement)
PORT=3000
EOF
